

@{
    ViewBag.Title = "Global Performance";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdminOSCE.cshtml";
}
@section css{
    @Scripts.Render("~/bundles/jquery")
}

<div class="">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Add Data</h3>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="alert alert-warning d-none  alert-dismissible  text-white" id="danger-alert" role="alert">
            <i class="fa fa-exclamation-triangle" style="font-size:20px" aria-hidden="true"></i>
            <span id="alertText" class="" style="font-size:20px"></span>
        </div>

        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Global Performance</h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <li>
                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                            </li>
                            <li>
                                <a class="close-link"><i class="fa fa-close"></i></a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <form class="" action="" method="post">
                            <div class="form-group row">
                                <h4 class="col-form-label  col-md-3 col-sm-3 ">Acad Career<span class="required text-danger"> * </span></h4>
                                <div class="col-sm-8">
                                    <select  name="ACADCareer" id="ACADCareer" class="form-control input-data">
                                        <option value="" selected disabled>Acad Career</option>
                                        @{
                                            foreach (var dt in ViewData["ACAD"] as IEnumerable<OSCEUKDI.Entities.ViewModel.VMDropDown>)
                                            {
                                                <option value="@dt.Nama">@dt.Nama</option>
                                            }
                                        }
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <h4 class="col-form-label  col-md-3 col-sm-3 ">Tahun Semester<span class="required text-danger"> * </span></h4>
                                <div class="col-sm-8">
                                    <select disabled name="TahunSemester" id="TahunSemester" class="form-control input-data">
                                        <option value="" selected disabled>Tahun Semester</option>
                                        @{
                                            foreach (var dt in ViewData["semester"] as IEnumerable<OSCEUKDI.Entities.ViewModel.VMDropDownTahunSemester>)
                                            {
                                                <option value="@dt.Nilai">@dt.Nama</option>
                                            }
                                        }
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <h4 class="col-form-label  col-md-3 col-sm-3 ">Judul Station<span class="required text-danger"> * </span></h4>
                                <div class="col-sm-8">
                                    <select disabled name="JudulStation" id="JudulStation" class="form-control fit input-data">
                                        <option value="" selected disabled>Judul Station</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <h4 class="col-form-label  col-md-3 col-sm-3 ">Nama<span class="required text-danger"> * </span></h4>
                                <div class="col-sm-8">
                                    <select disabled name="Nama" id="Nama" class="form-control input-data">

                                        @{
                                            foreach (var dt in ViewData["GlobalPerformance"] as IEnumerable<OSCEUKDI.Entities.ViewModel.VMDropDown>)
                                            {
                                                <option value="@dt.Nilai">@dt.Nama</option>
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <h4 class="col-form-label  col-md-3 col-sm-3 ">Performance<span class="required text-danger"> * </span></h4>
                                <div class="col-sm-2">
                                    <input disabled type="number" class="form-control" id="NMin" placeholder="Minimal" name="NMin">
                                </div> <strong class="mt-2">-</strong>
                                <div class="col-sm-2">
                                    <input disabled type="number" class="form-control" id="NMax" name="NMax" placeholder="Maximal">
                                </div>
                            </div>
                            <div class="ln_solid ">
                                <div class="form-group mt-3">
                                    <div class="col-md-6 offset-md-3">
                                        <button type='button' class="btn btn-round btn-success" onclick="PostCreate()">
                                            <i class="fa fa-save" aria-hidden="true"></i> Save
                                        </button>
                                        <button type='button' class="btn btn-round btn-secondary" onclick="history.back()">
                                            <i class="fa fa-arrow-circle-o-left" aria-hidden="true"></i> Back
                                        </button>
                                        <button type='reset' class="btn btn-round btn-danger">
                                            <i class="fa fa-refresh" aria-hidden="true"></i> Reset
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts{

    <script src="~/Scripts/Admin/GlobalPerformance/crud.js"></script>
}