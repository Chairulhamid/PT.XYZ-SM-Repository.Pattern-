@{
    ViewBag.Title = "Regression";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdminOSCE.cshtml";
}
@section css{
    @Scripts.Render("~/bundles/jquery")
}
<form action="@Url.Action("DownloadRegression","Regression")" method="post" id="DownloadRegression">
    <input type="hidden" id="TanggalUjian" name="TanggalUjian" />
    <input type="hidden" id="NomorStation" name="NomorStation" />
</form>
<div class="">
    <div class="page-title">
        <div class="title_left">
            <h3>Manage Regression</h3>
        </div>
    </div>
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel">
            <div class="x_title">
                <h2>List Regression</h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                    <li>
                        <a class="close-link"><i class="fa fa-close"></i></a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="card-box table-responsive">
                            <p class="text-muted font-13 m-b-30">
                            </p>
                            <table id="datatable-responsive" class="table table-striped bg-gray table-bordered dt-responsive nowrap table-Regression" cellspacing="0" width="100%">
                                <thead>
                                    <tr class="bg-green text-center">
                                        <th width="10%"><i class="fa fa-gear" aria-hidden="true" style="font-size:20px"></i></th>
                                        <th>No</th>
                                        <th>Tanggal Ujian</th>
                                        <th>Station</th>
                                        <th>Regression Linear</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Scripts/Admin/Regression/main.js"></script>
}


